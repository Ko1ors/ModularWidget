<UserControl x:Class="MusicWinModule.Views.MusicUC"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:MusicWinModule.Views" xmlns:fa="http://schemas.fontawesome.io/icons/"
             mc:Ignorable="d" 
             Height="80" Width="280">
    <Border BorderThickness="1,1,1,1" BorderBrush="Black" CornerRadius="10">
        <StackPanel  Orientation="Horizontal">
            <Image x:Name="thumbnail" Width="70" Height="70" Margin="20,0,5,0"/>
            <fa:ImageAwesome Icon="FastBackward" ></fa:ImageAwesome>
            </StackPanel>
    </Border>
</UserControl>
